{
   "env": {
      "browser": true,
      "commonjs": true,
      "es2021": true
   },
   "extends": ["eslint:recommended", "prettier"],
   "parserOptions": {
      "ecmaVersion": 11
   },
   "rules": {
      "no-unsafe-optional-chaining": "error",
      "no-unused-vars": ["error", { "ignoreRestSiblings": true }]
   },
   "globals": {
      "jest": true,
      "module": true,
      "require": true,
      "process": true
   }
}
